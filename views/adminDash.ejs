<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Admin Dashboard</title>
    <link rel="stylesheet" href="/admindash.css" />
    <style>
      .profList::-webkit-scrollbar {
        width: 0.1em;
      }

      .profList::-webkit-scrollbar-thumb {
        background-color: transparent;
      }
    </style>
  </head>
  <body>
    <div class="header">
      <a id="home" href="/"><strong>Home</strong></a>
      <!-- <a id="about" href="/About"><strong>About</strong></a> -->
    </div>
    <div class="line-design-1"></div>
    <div class="form-container">
      <form method="post" action="/admin/dashboard/createFaculty">
        <input name="name" type="text" placeholder="name" />
        <input name="email" type="email" placeholder="email" />
        <input name="password" type="password" placeholder="pass" />
        <button type="submit">Submit</button>
      </form>
    </div>
    <h1 id="timetable-rescheduler">TIMETABLE RESCHEDULER</h1>
    <div class="centered">
      <button id="add-prof">Add Prof</button>
      <button id="remove-prof">Remove Prof</button>
    </div>

    <ol class="profList"></ol>

    <script src="/scripts/adminDash.js" type="text/javascript"></script>
  </body>
</html>
